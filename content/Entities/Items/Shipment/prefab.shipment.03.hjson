$name: "Shipment"
$desc:
	'''
	A large shipping crate used for storage and transport of various goods.
	'''
$tags: []

body:
{
	inventory_weight_multiplier: 0.200

	@shape.box:
	{
		size: [3.500, 2.000]
		radius: 0.250
		mass: 170.000
		friction: 0.750
		elasticity: 0.500
		layer: entity, crate, shipment, solid, flammable
		mask: world, crate
		material: wood
	}
}

animated.renderer:
{
	sprite:
	{
		texture: "shipment.03"
	}
	z: -90.000
}

transform:
{

}

interactable:
{
	window_size: [192.000, 192.000]
}

holdable:
{
	force_multiplier: 1.000
	torque_multiplier: 0.500

	radius: 0.000
	flags: disable_parent_facing
}

attachment:
{
	force_multiplier: 1.000
	torque_multiplier: 4.000
	offset: [-0.500, 1.125]
	flags: disable_parent_facing
	tags: none
}