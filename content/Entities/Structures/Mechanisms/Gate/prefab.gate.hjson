$tags: []

body:
{
	type: static
	
	@shape.box:
	{
		size: [1.000, 12.000]
		offset: [0.000, 0.000]
		friction: 0.990
		elasticity: 0.030
		material: wood
		layer: entity, building, ignore_melee, flammable
		mask: world
	}
}

health:
{
	max: 40000.000
}

animated.renderer:
{
	sprite:
	{
		texture: "gate.base"
	}
	z: -150.000
}

gate:
{
	length: 6.000
}

axle:
{
	offset: [-1.000, -5.500]
	mass: 5.000
	friction: 0.800
	radius_a: 1.000
	radius_b: 1.000
	radius_inner: 0.750
	radius_outer: 1.000
	z: -50.000
	
	@animated.renderer:
	{
		sprite:
		{
			texture: "wheel_1000.steel"
		}
	}
}

transform:
{

}

faction:
{
	
}

interactable:
{

}

